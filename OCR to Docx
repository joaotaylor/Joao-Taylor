    {
      "cell_type": "code",
      "source": [
        "import os\n",
        "from pdf2image import convert_from_path\n",
        "import pytesseract\n",
        "from docx import Document\n",
        "\n",
        "def pdf_to_word(pdf_path, output_word_path):\n",
        "    # Converte o PDF em imagens. Aumente o dpi para melhorar a qualidade do OCR, se necessário.\n",
        "    pages = convert_from_path(pdf_path, dpi=300)\n",
        "\n",
        "    # Cria um novo documento Word\n",
        "    document = Document()\n",
        "\n",
        "    # Itera sobre cada página convertida\n",
        "    for page_number, page in enumerate(pages, start=1):\n",
        "        # Extrai o texto da imagem usando OCR.\n",
        "        # Certifique-se de que o idioma esteja configurado corretamente (aqui usamos 'por' para português).\n",
        "        text = pytesseract.image_to_string(page, lang='deu')\n",
        "\n",
        "        # Adiciona um título para identificar a página (opcional)\n",
        "        document.add_heading(f'Página {page_number}', level=2)\n",
        "\n",
        "        # Adiciona o texto extraído como um parágrafo no documento\n",
        "        document.add_paragraph(text)\n",
        "\n",
        "    # Salva o documento Word\n",
        "    document.save(output_word_path)\n",
        "    print(f\"Documento salvo em: {output_word_path}\")\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    # Caminho do PDF escaneado\n",
        "    pdf_path = '20211209 AVV Art. 28 Abs 3 DS-GVO_plenum_MAXWORX.pdf'\n",
        "\n",
        "    # Caminho de saída para o arquivo Word. Note que estamos gerando um .docx.\n",
        "    # Caso seja estritamente necessário o formato .doc, você poderá converter posteriormente.\n",
        "    output_word_path = 'C:\\\\Users\\\\JoaoTaylor\\\\Downloads\\\\saida.docx'\n",
        "\n",
        "    pdf_to_word(pdf_path, output_word_path)"
      ],
