      "cell_type": "code",
      "source": [
        "# Instale o Tesseract e o pacote para alemão\n",
        "!apt-get update\n",
        "!apt-get install -y tesseract-ocr tesseract-ocr-deu\n",
        "\n",
        "# Configure a variável de ambiente para o Tesseract (opcional, se necessário)\n",
        "import os\n",
        "os.environ['TESSDATA_PREFIX'] = '/usr/share/tesseract-ocr/4.00/tessdata/'\n",
        "\n",
        "# Código para converter PDF e extrair texto\n",
        "from pdf2image import convert_from_path\n",
        "import pytesseract\n",
        "from docx import Document\n",
        "\n",
        "def pdf_to_word(pdf_path, output_word_path):\n",
        "    pages = convert_from_path(pdf_path, dpi=300)\n",
        "    document = Document()\n",
        "\n",
        "    for page_number, page in enumerate(pages, start=1):\n",
        "        # Utilizando o idioma alemão ('deu')\n",
        "        text = pytesseract.image_to_string(page, lang='deu')\n",
        "        document.add_heading(f'Página {page_number}', level=2)\n",
        "        document.add_paragraph(text)\n",
        "\n",
        "    document.save(output_word_path)\n",
        "    print(f\"Documento salvo em: {output_word_path}\")\n",
        "\n",
        "# Exemplo de caminhos no Colab (certifique-se de que o PDF foi enviado para o Colab)\n",
        "pdf_path = '/content/seu_arquivo.pdf'         # Substitua pelo nome do seu arquivo PDF\n",
        "output_word_path = '/content/saida.docx'\n",
        "\n",
        "pdf_to_word(pdf_path, output_word_path)\n"
      ],
