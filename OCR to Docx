      "source": [
        "from google.colab import files\n",
        "uploaded = files.upload()\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 110
        },
        "id": "J0poNlfmxiKK",
        "outputId": "2c36caa0-80c8-4732-8891-06d35bc965f5"
